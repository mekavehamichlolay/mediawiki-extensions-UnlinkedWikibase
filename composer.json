{
	"require-dev": {
		"mediawiki/mediawiki-codesniffer": "^39.0",
		"mediawiki/minus-x": "^1.0",
		"php-parallel-lint/php-parallel-lint": "^1.0"
	},
	"scripts": {
		"test": [
			"parallel-lint . --exclude vendor",
			"@phpcs",
			"minus-x check ."
		],
		"fix": [
			"phpcbf"
		],
		"phpcs": "phpcs -sp --cache"
	}
}
